<span class="n">debug</span><span class="p">:</span> <span class="n">Appium</span> <span class="n">request</span> <span class="n">initiated</span> <span class="n">at</span> <span class="o">/</span><span class="n">wd</span><span class="o">/</span><span class="n">hub</span><span class="o">/</span><span class="n">session</span><span class="o">/</span><span class="mi">75108063</span><span class="o">-</span><span class="n">b03d</span><span class="o">-</span><span class="mi">42</span><span class="n">f1</span><span class="o">-</span><span class="mi">9922</span><span class="o">-</span><span class="mi">0</span><span class="n">b31b641cf3e</span><span class="o">/</span><span class="n">touch</span><span class="o">/</span><span class="n">perform</span>

