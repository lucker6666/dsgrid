<pre class="highlight ruby"><span class="k">def</span> <span class="nf">yingqilai</span><span class="p">(</span><span class="n">myself_money</span><span class="p">,</span><span class="n">girlfriend_money</span><span class="p">):</span>
    <span class="k">if</span> <span class="n">girlfriend_money</span><span class="o">&gt;</span><span class="ss">myself_money:
       </span><span class="n">new_girlfriend</span> <span class="o">=</span> <span class="n">fenshou</span><span class="p">.</span><span class="nf">changeGirlFriend</span><span class="p">()</span>
       <span class="k">if</span> <span class="n">new_girlfriend</span><span class="p">.</span><span class="nf">money</span><span class="p">()</span><span class="o">&lt;</span><span class="ss">myself_money:
            </span><span class="n">marry</span><span class="p">()</span>
</pre>

