<span class="kn">import</span> <span class="nn">android.widget.ListView</span><span class="o">;</span>

<span class="kd">public</span> <span class="kd">class</span> <span class="nc">AppListActivity</span> <span class="kd">extends</span> <span class="n">Activity</span> <span class="o">{</span>
    <span class="cm">/** Called when the activity is first created. */</span>
