    <span class="kd">private</span> <span class="kd">static</span> <span class="n">Class</span> <span class="n">launcherActivityClass</span><span class="o">;</span>

    <span class="kd">static</span><span class="o">{</span>
        <span class="n">File</span> <span class="n">file</span><span class="o">=</span><span class="k">new</span> <span class="n">File</span><span class="o">(</span><span class="s">"/mnt/sdcard"</span><span class="o">,</span> <span class="s">"activityName.txt"</span><span class="o">);</span>

        <span class="k">try</span> <span class="o">{</span>           
