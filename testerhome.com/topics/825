      <div class="body">
        <p><a href="825#reply3" class="at_floor" data-floor="3">#3楼</a> <a href="../anikikun" class="at_user" title="@anikikun"><i>@</i>anikikun</a> <br>
// new 1.0 vison<br>
        DesiredCapabilities capabilities = new DesiredCapabilities();<br>
        capabilities.setCapability(CapabilityType.BROWSER_NAME, "");<br>
        capabilities.setCapability(CapabilityType.VERSION, "4.1.2");<br>
        capabilities.setCapability("device", "Selendroid"); //Selendroid   Android<br>
        capabilities.setCapability("app", app.getAbsolutePath());<br>
        capabilities.setCapability("appPackage", "android.view");<br>
        capabilities.setCapability("appActivity", "HomeActivity");<br>
        driver = new AppiumDriver(new URL("<a href="http://127.0.0.1:4723/wd/hub" rel="nofollow" target="_blank">http://127.0.0.1:4723/wd/hub</a>"), capabilities);</p>
      </div>
