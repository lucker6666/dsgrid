<span class="n">GET</span> <span class="o">/</span><span class="n">wd</span><span class="o">/</span><span class="n">hub</span><span class="o">/</span><span class="n">session</span><span class="o">/</span><span class="n">bd62b7a5</span><span class="o">-</span><span class="n">c2dd</span><span class="o">-</span><span class="mi">4545</span><span class="o">-</span><span class="mi">9</span><span class="n">f56</span><span class="o">-</span><span class="mi">594957055823</span> <span class="mi">200</span> <span class="mi">9</span><span class="n">ms</span> <span class="o">-</span> <span class="mi">388</span><span class="n">b</span>
<span class="nl">debug:</span> <span class="n">Appium</span> <span class="n">request</span> <span class="n">initiated</span> <span class="n">at</span> <span class="o">/</span><span class="n">wd</span><span class="o">/</span><span class="n">hub</span><span class="o">/</span><span class="n">session</span><span class="o">/</span><span class="n">bd62b7a5</span><span class="o">-</span><span class="n">c2dd</span><span class="o">-</span><span class="mi">4545</span><span class="o">-</span><span class="mi">9</span><span class="n">f56</span><span class="o">-</span><span class="mi">594957055823</span><span class="o">/</span><span class="n">wind</span>
<span class="n">ow</span>
<span class="nl">debug:</span> <span class="n">Request</span> <span class="n">received</span> <span class="n">with</span> <span class="nl">params:</span> <span class="o">{</span><span class="s">"name"</span><span class="o">:</span><span class="s">"WEBVIEW"</span><span class="o">}</span>
<span class="nl">info:</span> <span class="n">Responding</span> <span class="n">to</span> <span class="n">client</span> <span class="n">that</span> <span class="n">a</span> <span class="n">method</span> <span class="n">is</span> <span class="n">not</span> <span class="n">implemented</span>
